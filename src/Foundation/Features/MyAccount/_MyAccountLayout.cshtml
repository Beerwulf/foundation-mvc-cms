@inject Foundation.Features.MyAccount.NavigationService navigationService
@model IContentViewModel<IContent>

@{
    Layout = "~/Features/Shared/Views/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12 col-lg-3">
        @{
            var model = navigationService.MyAccountMenu();
            await Html.RenderPartialAsync("_ProfileSidebar", model);
        }
    </div>
    <div class="col-md-12 col-lg-9">
        @RenderBody()
    </div>
</div>